@model IEnumerable<TSHis2.Models.Diagnosis>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Examiation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diagnosis1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DoctorDecision)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiagnosisDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiagnosisHour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiagnosisLocation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Emp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Visit)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Examiation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diagnosis1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoctorDecision)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiagnosisDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiagnosisHour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiagnosisLocation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Emp.EmpId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Visit.VisitId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DiagnosisId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DiagnosisId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DiagnosisId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
